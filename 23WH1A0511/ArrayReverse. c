#include<stdio.h>
void main(){
    int num[30],range;
    printf("enter the number of values to be given: ");
    scanf("%d",&range);
    printf("enter any %d values: ",range);
    for(int i=0;i<range;i++){
        scanf("%d",&num[i]);
    }
    for(int j=range-1;j>=0;j--){
        printf("%d ",num[j]);
    }
}

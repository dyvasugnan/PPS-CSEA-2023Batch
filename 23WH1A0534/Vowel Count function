#include <stdio.h>
int vowelcount(char []);
int main()
{
    char str[20],count;
    scanf("%s",str);
    count=vowelcount(str);
    printf("%d",count);
    return 0;
}
int vowelcount(char str[20])
{
    int i,c=0;
    for(i=0; str[i] != '\0';i++)
    {
        if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u')
        {
            c++;
        }
    }
    return c;
}


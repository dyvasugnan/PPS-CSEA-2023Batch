#include<stdio.h>
int main()
{
int m1,m2,m3,m4,m5,m5,total;
float avg;
printf("enter m1,m2,m3,m4,m5,m6 values : ");
scanf("%d%d%d%d%d%d",&m1,&m2,&m3,&m4,&m5,&m6);
total=m1+m2+m3+m4+m5+m6;
int avg=(total)/6;
if(avg>=70)
{
printf("avg is distinction");
}
else if(avg>=60&avg<70)
{
printf("avg is 1st class");
}
else if(avg>=40&avg<60)
{
printf("avg is 2nd class");
}
else if(avg<40)
{
printf("avg is fail");
}
}
